# BEGIN_KITTY_FONTS
font_family      iMWritingMono Nerd Font Mono
italic_font      auto
bold_font        auto
bold_italic_font auto

hide_window_decorations yes

enable_audio_bell no

# BEGIN_KITTY_THEME
# Cyberpunk
include current-theme.conf
# END_KITTY_THEME

shell_integration no-cursor
cursor_shape block
cursor_blink_interval 0
